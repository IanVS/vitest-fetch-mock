{
  "compilerOptions": {
    "module": "es2020",
    "moduleResolution": "node",
    "lib": ["es2015", "dom"],
    "baseUrl": ".",
    "paths": {
      "vitest-fetch-mock": ["."]
    },
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true
  }
}
